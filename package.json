{
  "name": "telegram-detailing-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" --names \"CLIENT,SERVER\" --prefix-colors \"cyan,magenta\"",
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "build": "tsc && vite build",
    "build:server": "cd server && npm install",
    "preview": "vite preview",
    "install:all": "npm install && cd server && npm install",
    "start": "concurrently \"npm run preview\" \"npm run server\" --names \"CLIENT,SERVER\" --prefix-colors \"cyan,magenta\"",
    "server": "cd server && npm start"
  },
  "dependencies": {
    "@twa-dev/sdk": "^7.10.1",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.468.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.4.3",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.7"
  }
}
