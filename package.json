{
  "name": "telegram-detailing-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" --names \"CLIENT,SERVER\" --prefix-colors \"cyan,magenta\"",
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "build": "tsc && vite build",
    "build:server": "cd server && npm install",
    "preview": "vite preview",
    "install:all": "npm install && cd server && npm install",
    "start": "concurrently \"npm run preview\" \"npm run server\" --names \"CLIENT,SERVER\" --prefix-colors \"cyan,magenta\"",
    "server": "cd server && npm start",
    "seed:services": "cd server && node scripts/seed-services.js",
    "seed:gallery": "cd server && node scripts/seed-gallery.js",
    "init:settings": "cd server && node scripts/init-settings.js",
    "seed:all": "npm run seed:services && npm run seed:gallery && npm run init:settings"
  },
  "dependencies": {
    "@twa-dev/sdk": "^7.10.1",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "framer-motion": "^11.15.0",
    "google-auth-library": "^9.6.3",
    "google-spreadsheet": "^4.1.2",
    "googleapis": "^171.1.0",
    "lucide-react": "^0.468.0",
    "node-cache": "^5.1.2",
    "react": "^18.3.1",
    "react-day-picker": "^9.4.3",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "telegraf": "^4.16.3",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.7"
  }
}
